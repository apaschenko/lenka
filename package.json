{
  "name": "lenka",
  "version": "0.2.17",
  "description": "Typescript and javascript utilities. deepCopy: deep clone the objects with support circular references and customizing.",
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "/lib",
    "/src"
  ],
  "scripts": {
    "build": "npx tsc --project tsconfig.json && npm run coverage && npm run refresh:readme && npm run cleanup",
    "test": "npx mocha lib/test/**",
    "coverage": "npx c8 -c=.c8.json npm test",
    "refresh:readme": "node lib/utils/includeFilesToREADME.js",
    "cleanup": "rm lib/utils/*.d.ts lib/ts_examples/**/*.d.ts lib/tsconfig.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apaschenko/lenka.git"
  },
  "keywords": [
    "deepcopy",
    "deep copy",
    "custom copy",
    "deep clone",
    "circular dependencies",
    "circular deps",
    "circular references",
    "circular",
    "typescript",
    "es6"
  ],
  "author": "Alexander Paschenko",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/apaschenko/lenka/issues"
  },
  "homepage": "https://github.com/apaschenko/lenka#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "app-root-path": "^3.0.0",
    "c8": "^7.11.0",
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "deep-copy-system": "^2.0.0",
    "mocha": "^9.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "dependencies": {   
  }
}
